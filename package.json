{
  "name": "is-travis-ci-build",
  "description": "Check if your code is running on Travis-CI build.",
  "version": "1.0.0",
  "author": {
    "name": "sanemat",
    "email": "o.gata.ken@gmail.com",
    "url": "sane.jp"
  },
  "bin": "cli.js",
  "dependencies": {
    "boolify-string": "^1.0.1",
    "meow": "^3.3.0"
  },
  "devDependencies": {
    "conventional-changelog": "^0.1.0-beta",
    "david": "^6.1.6",
    "eclint": "^0.2.6",
    "eslint": "^1.0.0-rc",
    "espower-loader": "^0.11.0",
    "fixpack": "^2.2.0",
    "globstar": "^1.0.0",
    "mocha": "^2.2.5",
    "power-assert": "^0.11.0",
    "touch": "^1.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "ci",
    "cli",
    "cli-app",
    "travis",
    "travis-ci"
  ],
  "license": "MIT",
  "repository": "pandawing/node-is-travis-ci-build",
  "scripts": {
    "changelog": "touch changelog.md && conventional-changelog -i changelog.md --overwrite --preset angular",
    "david": "david update",
    "eclint": "globstar --ignore \"node_modules/**\" -- eclint check \"*.{js,yml,md,json}\"",
    "eslint": "eslint .",
    "fixpack": "fixpack",
    "git:tag": "git tag v${npm_package_version}",
    "lint": "npm run eclint && npm run eslint",
    "test": "mocha --require ./enable-power-assert.js -- ./test.js",
    "verify": "npm run lint && npm run test"
  }
}
